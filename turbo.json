{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "next-env.d.ts", ".expo/**"]
    },
    "lint": {},
    "lint:fix": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "cache": false
    }
  },
  "globalEnv": [
    "MIGRATIONS_TURSO_ORG_SLUG",
    "MIGRATIONS_TURSO_API_BASE_URL",
    "MIGRATIONS_TURSO_ORG_DB_GROUPS",
    "MIGRATIONS_TURSO_AUTH_TOKEN",
    "TRPC_TURSO_ORG_SLUG",
    "TRPC_TURSO_API_BASE_URL",
    "TRPC_TURSO_AUTH_TOKEN",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "CLERK_SECRET_KEY",
    "CLERK_WEBHOOK_SECRET_KEY",
    "NEXT_PUBLIC_BASE_URL",
    "NEXT_PUBLIC_CLERK_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_SIGN_UP_URL",
    "NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL",
    "OPENAI_API_KEY"
  ]
}
